import solutions.Fetch;

func find(i) {
   Fetch.get(6)
      .reader()
      .readText()
      .sliding(i)
      .elements()
      .stream()
      .filter(s -> s.value.distinct.length == i)
      .findFirst()
      .map(s -> s.source.indexOf(s.value) + i)
      .get();
}

println("${find(4)} ${find(14)}");